{:webly {:ns-clj [goldly.app]}
 :keybindings []

  :nrepl {:enabled true
          :bind "0.0.0.0"
          :port 9100}

 :goldly {; build
          :lazy true
          :lazy-exclude #{"ui-binaryclock"  ; test for excluding lazy extensions from lazy build
                          "ui-math"}
          ; run-time
          :autoload-clj-ns [demo.service]
          :autoload-cljs-dir ["demo/cljs_libs"
                              "demo/page"]
          :routes {:app {"" :user/main
                         "scicompile" :user/scicompile
                         "lazy" :user/lazy
                         "error" :user/error
                         "select" :user/select
                         "service" :user/service

                         ;devtools
                         "devtools/help" :devtools
                         "devtools/pages" :pages
                         "devtools/scratchpad" :scratchpad
                         "devtools/viewer" :viewer
                         "devtools/environment" :environment
                         "devtools/theme" :theme}
                   :api {"rdocument/ns" :rdocument/ns
                         "rdocument/files" :rdocument/files
                         ["rdocument/file/" :ns "/" :name] :rdocument/file}}}

 :devtools {:rdocument  {:storage-root "rdocument/"
                         :url-root "/api/rdocument/file/"}
            :collections {:demo [:clj "demo/notebook/"]}}

;
 }
